<mvc:View controllerName="ehsmehsm.controller.EHSMLoginPage"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
     <Page id="page" title="{i18n>title}">
        <content>
						
					<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
					<ToolbarSpacer id="_IDGenToolbarSpacer2"/>
					<VBox id="_IDGenVBox1" justifyContent="Center" alignItems="Center" alignContent="Center" class="middlecontent">
						<items>
							<Label id="_IDGenLabel1" text="Use your credentials" class="Topoftable"/>
							<Label id="_IDGenLabel2" text="UserId" class="userid"></Label>
							<Input id="name" placeholder="Enter your UserId"/>
							<Label id="_IDGenLabel3" text="Password" class="password"></Label>
							<Input id="password" placeholder="Enter your Password" type="Password"/>
							<Button id="_IDGenButton1" width="4rem" text="Login" type="Emphasized" press="onPress"></Button>
						</items>
					</VBox>
					
				</content>
    </Page>
</mvc:View>
